<div class="report">
<a href="https://infoarena.ro/forum?action=post;topic=403">Raporteaza aceasta sursa</a>
</div>
<div class="code"><pre><code class="hljs cpp"><span class="hljs-meta">#<span class="hljs-meta-keyword">include</span> <span class="hljs-meta-string">&lt;fstream&gt;</span></span>
<span class="hljs-keyword">using</span> <span class="hljs-keyword">namespace</span> <span class="hljs-built_in">std</span>;
<span class="hljs-function">ifstream <span class="hljs-title">in</span><span class="hljs-params">(<span class="hljs-string">"perechi.in"</span>)</span></span>;
<span class="hljs-function">ofstream <span class="hljs-title">out</span><span class="hljs-params">(<span class="hljs-string">"perechi.out"</span>)</span></span>;
<span class="hljs-function"><span class="hljs-keyword">int</span> <span class="hljs-title">main</span><span class="hljs-params">()</span>
</span>{
    <span class="hljs-keyword">int</span> n,d,e,ans=<span class="hljs-number">1</span>;
    in&gt;&gt;n;
    <span class="hljs-keyword">if</span>(n&gt;<span class="hljs-number">1</span>)
    {
        d=<span class="hljs-number">2</span>;
        <span class="hljs-keyword">while</span>(n&gt;<span class="hljs-number">1</span> &amp;&amp; d*d&lt;=n)
        {
            e=<span class="hljs-number">0</span>;
            <span class="hljs-keyword">while</span>(n%d==<span class="hljs-number">0</span>)
                e++,n/=d;
            <span class="hljs-keyword">if</span>(e&gt;<span class="hljs-number">0</span>)
            {
                ans=ans*(<span class="hljs-number">2</span>*e+<span class="hljs-number">1</span>);
            }
            d++;
        }
        <span class="hljs-keyword">if</span>(n&gt;<span class="hljs-number">1</span>)
            ans=ans*(<span class="hljs-number">2</span>*<span class="hljs-number">1</span>+<span class="hljs-number">1</span>);
    }
    ans=(ans+<span class="hljs-number">1</span>)/<span class="hljs-number">2</span>;
    out&lt;&lt;ans;
    <span class="hljs-keyword">return</span> <span class="hljs-number">0</span>;
}
</code></pre></div></div>
